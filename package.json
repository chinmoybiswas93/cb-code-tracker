{
  "name": "cb-code-tracker",
  "displayName": "cb-code-tracker",
  "description": "'Personal coding time tracker'",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "startup"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cb-code-tracker.sts",
        "title": "Show Todays Stats"
      },
      {
        "command": "cb-code-tracker.showTime",
        "title": "Show Current Time"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.99.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.23.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
